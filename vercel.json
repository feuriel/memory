{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/assets/(.*)\\.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
